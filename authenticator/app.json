{
  "expo": {
    "name": "MOS Authenticator",
    "slug": "open-auth",
    "owner": "opensrc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "5",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Authenticator needs access to your Camera."
      },
      "bundleIdentifier": "org.makeopensource.authenticator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "org.makeopensource.authenticator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
